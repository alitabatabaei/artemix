image = {
    
}

init = ->
    rcanvas = $("#right_cover")[0]
    width = rcanvas.width
    height = rcanvas.height

    rcontext = rcanvas.getContext('2d')
    rimg = new Image()

    rimg.onload = ->
        rcontext.drawImage(rimg, 0, 0, width, height)

    rimg.src = "<%= asset_path 'right_leg.png' %>"


    lcontext = $('#left_cover')[0].getContext('2d')
    limg = new Image()

    limg.onload = ->
        lcontext.drawImage(limg, 0, 0, width, height)

    limg.src = "<%= asset_path 'left_leg.png' %>"

$('document').on('page:load', init)
$ -> 
    init()